{
  "name": "webpack-2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prod": "better-npm-run build-prod",
    "dev": "better-npm-run build-dev"
  },
  "betterScripts": {
    "build-prod": {
      "command": "./node_modules/.bin/webpack --profile --config webpack/webpack.prod.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-dev": {
      "command": "./node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 8080
      }
    }
  },
  "author": "Stepan",
  "license": "MIT",
  "devDependencies": {
