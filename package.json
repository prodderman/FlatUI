{
  "name": "webpack-2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prod": "better-npm-run build-prod",
    "dev": "better-npm-run build-dev"
  },
  "betterScripts": {
    "build-prod": {
      "command": "./node_modules/.bin/webpack --profile --config webpack/webpack.prod.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-dev": {
      "command": "./node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 8080
      }
    }
  },
  "author": "Stepan",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery-pjax": "^2.0.1",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.3.0",
    "slideout": "^1.0.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "swiper": "^3.4.2",
    "webpack-config": "^7.1.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1"
  },
  "dependencies": {
    "cssnano": "^3.10.0",
    "jquery": "^3.2.1",
    "jquery-circle-progress": "^1.2.2",
    "jquery-form-styler": "^2.0.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-slider-pips": "^1.11.4",
