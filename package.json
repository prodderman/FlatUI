{
  "name": "webpack-2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "better-npm-run build-test",
    "prod": "better-npm-run build-prod",
    "dev": "better-npm-run build-dev",
    "P": "node nPage.js",
    "C": "node nComp.js"
  },
  "betterScripts": {
    "build-prod": {
      "command": "webpack --display-modules --json --profile >stats.json",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-test": {
      "command": "webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-dev": {
      "command": "webpack-dev-server --hotOnly",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 8080
      }
    }
  },
  "author": "Stepan",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.3",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.4.1",
    "webpack-config": "^7.1.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "jquery-circle-progress": "^1.2.2",
    "jquery-form-styler": "^2.0.1",
    "jquery-ui": "^1.12.1",
